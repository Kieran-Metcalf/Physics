'''
***** python3 ****
particle physics card game
'''

class Card:

        def __init__ (self, name, spin, ):
                self.name = name
                self.spin = spin

        def __repr__ (self):
                return self.name + " with spin " + self.spin

my_card = Card("electron","up")
print(my_card)
my_card = Card("electron","down")
print(my_card)

